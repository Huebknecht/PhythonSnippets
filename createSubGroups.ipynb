   "source": [
    "\n",
    "# create sub-groups\n",
    "\n",
    "# dependency\n",
    "from functools import reduce\n",
    "\n",
    "# data\n",
    "s = (1, 2, 3, 4, 5, 6)\n",
    "\n",
    "# one-liner\n",
    "ps = lambda s: reduce(lambda P, x: P + [subset | {x} for subset in P], s, [set()])\n",
    "\n",
    "# call\n",
    "print(ps(s))\n",
    "sA = ps(s)\n",
    "print(sA[1])\n",
    "# print every second\n",
    "print(sA[1::2])\n"
   ]
